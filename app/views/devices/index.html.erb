<p id="notice"><%= notice %></p>

<h1>Listing Devices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Device ID</th>
    </tr>
  </thead>

  <tbody>
    <% devices.each do |device| %>
      <tr>
        <td><%= device.name  %></td>
        <td><%= device.device_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', url_for(:back)  %>
